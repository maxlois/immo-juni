{% extends 'base.html.twig' %}

{% block title %}Location{% endblock %}

{% block body %}
    <h1>Location</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ location.id }}</td>
            </tr>
            <tr>
                <th>DateD_location</th>
                <td>{{ location.dateDLocation ? location.dateDLocation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Penalite</th>
                <td>{{ location.penalite }}</td>
            </tr>
            <tr>
                <th>DelaisPaiem</th>
                <td>{{ location.delaisPaiem }}</td>
            </tr>
            <tr>
                <th>CausionEnt</th>
                <td>{{ location.causionEnt }}</td>
            </tr>
            <tr>
                <th>CausionSort</th>
                <td>{{ location.causionSort }}</td>
            </tr>
            <tr>
                <th>EtatLieu</th>
                <td>{{ location.etatLieu }}</td>
            </tr>
            <tr>
                <th>DateL</th>
                <td>{{ location.dateL ? location.dateL|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_location_index') }}">back to list</a>

    <a href="{{ path('app_location_edit', {'id': location.id}) }}">edit</a>

    {{ include('location/_delete_form.html.twig') }}
